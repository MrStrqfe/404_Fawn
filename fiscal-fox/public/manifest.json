{
  "manifest_version": 3,
  "name": "Fiscal Fox",
  "version": "1.0.0",
  "description": "Fiscal Fox Chrome Extension",
  "permissions": ["activeTab", "scripting"],
  "options_page": "settings.html",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
